RUN |3 NUCLIO_LABEL=1.11.21 NUCLIO_ARCH=amd64 NUCLIO_BUILD_LOCAL_HANDLER_DIR=handler /bin/sh -c apt update && apt install --no-install-recommends -y libglib2.0-0 && apt install wget # buildkit
USER root
ARG NUCLIO_BUILD_LOCAL_HANDLER_DIR
ARG NUCLIO_ARCH
ARG NUCLIO_LABEL

ENV DEBIAN_FRONTEND=teletype
RUN /bin/sh -c git clone hhttps://github.com/ultralytics/ultralytics /usr/src/ultralytics/ # buildkit
WORKDIR /usr/src/ultralytics/
RUN /bin/sh -c mkdir -p /usr/src/ultralytics/ # buildkit
RUN /bin/sh -c pip install --no-cache -r requirements.txt albumentations gsutil notebook     coremltools onnx onnx-simplifier onnxruntime 'openvino-dev>=2022.3'     --extra-index-url https://download.pytorch.org/whl/cpu # buildkit
