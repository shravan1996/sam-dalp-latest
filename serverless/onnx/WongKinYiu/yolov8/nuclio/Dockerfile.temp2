COPY handler /opt/nuclio # buildkit
COPY artifacts/py3.8-whl /opt/nuclio/whl # buildkit
COPY artifacts/py /opt/nuclio/ # buildkit
COPY artifacts/processor /usr/local/bin/processor # buildkit
RUN |3 NUCLIO_LABEL=1.11.21 NUCLIO_ARCH=amd64 NUCLIO_BUILD_LOCAL_HANDLER_DIR=handler /bin/sh -c ln -s /usr/bin/python3 /usr/bin/python # buildkit
RUN |3 NUCLIO_LABEL=1.11.21 NUCLIO_ARCH=amd64 NUCLIO_BUILD_LOCAL_HANDLER_DIR=handler /bin/sh -c wget https://github.com/ultralytics/assets/releases/download/v0.0.0/yolov8n.pt # buildkit
WORKDIR /opt/nuclio
